{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "generated-page.html",
      "use": "@vercel/static"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/test.py" },
    { "src": "/visualizations", "dest": "/web/visualizations.html" },
    { "src": "/research", "dest": "/web/research.html" },
    { "src": "/account", "dest": "/web/account.html" },
    { "src": "/", "dest": "/generated-page.html" }
  ]
}

