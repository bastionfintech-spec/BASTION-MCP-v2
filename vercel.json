{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/main.py" },
    { "src": "/visualizations", "dest": "/web/visualizations.html" },
    { "src": "/research", "dest": "/web/research.html" },
    { "src": "/account", "dest": "/web/account.html" },
    { "src": "/frontend", "dest": "/BASTION FRONT END.html" },
    { "src": "/", "dest": "/api/main.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}

