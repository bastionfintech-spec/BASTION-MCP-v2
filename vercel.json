{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/terminal_api.py" },
    { "source": "/visualizations", "destination": "/web/visualizations.html" },
    { "source": "/research", "destination": "/web/research.html" },
    { "source": "/account", "destination": "/web/account.html" },
    { "source": "/frontend", "destination": "/BASTION FRONT END.html" }
  ],
  "functions": {
    "api/terminal_api.py": {
      "runtime": "python3.12",
      "maxDuration": 30
    }
  }
}

